{
  "multipart": [
    {
      "when": {
        "type": "post"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/post"
      }
    },
    {
      "when": {
        "type": "post_top"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/post_top"
      }
    },
    {
      "when": {
        "parm_north": "true"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/post_arm"
      }
    },
    {
      "when": {
        "parm_east": "true"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/post_arm",
        "y": 90
      }
    },
    {
      "when": {
        "parm_south": "true"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/post_arm",
        "y": 180
      }
    },
    {
      "when": {
        "parm_west": "true"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/post_arm",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "north",
        "flipstate": "up",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm"
      }
    },
    {
      "when": {
        "facing": "north",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_double"
      }
    },
    {
      "when": {
        "facing": "east",
        "flipstate": "up",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "east",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_double",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "flipstate": "up",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "south",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_double",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "flipstate": "up",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "west",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_double",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "north",
        "flipstate": "down",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm",
        "x": 180,
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east",
        "flipstate": "down",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm",
        "x": 180,
        "y": 270
      }
    },
    {
      "when": {
        "facing": "south",
        "flipstate": "down",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm",
        "x": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "flipstate": "down",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm",
        "x": 180,
        "y": 90
      }
    },
    {
      "when": {
        "facing": "north",
        "flipstate": "both",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_twoway"
      }
    },
    {
      "when": {
        "facing": "east",
        "flipstate": "both",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_twoway",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "flipstate": "both",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_twoway",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "flipstate": "both",
        "type": "arm"
      },
      "apply": {
        "model": "immersiveposts:nickelpost/arm_twoway",
        "y": 270
      }
    },
    {
      "when": {
        "type": "empty"
      },
      "apply": {
        "model": "immersiveposts:block/empty"
      }
    }
  ]
}