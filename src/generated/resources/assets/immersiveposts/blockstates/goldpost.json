{
  "multipart": [
    {
      "when": {
        "type": "post"
      },
      "apply": {
        "model": "immersiveposts:goldpost/post"
      }
    },
    {
      "when": {
        "type": "post_top"
      },
      "apply": {
        "model": "immersiveposts:goldpost/post_top"
      }
    },
    {
      "when": {
        "parm_north": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/post_arm"
      }
    },
    {
      "when": {
        "parm_east": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/post_arm",
        "y": 90
      }
    },
    {
      "when": {
        "parm_south": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/post_arm",
        "y": 180
      }
    },
    {
      "when": {
        "parm_west": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/post_arm",
        "y": -90
      }
    },
    {
      "when": {
        "facing": "north",
        "type": "arm",
        "flip": "false"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm"
      }
    },
    {
      "when": {
        "facing": "north",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm_double"
      }
    },
    {
      "when": {
        "facing": "east",
        "type": "arm",
        "flip": "false"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "east",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm_double",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "type": "arm",
        "flip": "false"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "south",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm_double",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "type": "arm",
        "flip": "false"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm",
        "y": -90
      }
    },
    {
      "when": {
        "facing": "west",
        "type": "arm_double"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm_double",
        "y": -90
      }
    },
    {
      "when": {
        "facing": "north",
        "type": "arm",
        "flip": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm",
        "x": 180,
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east",
        "type": "arm",
        "flip": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm",
        "x": 180,
        "y": -90
      }
    },
    {
      "when": {
        "facing": "south",
        "type": "arm",
        "flip": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm",
        "x": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "type": "arm",
        "flip": "true"
      },
      "apply": {
        "model": "immersiveposts:goldpost/arm",
        "x": 180,
        "y": 90
      }
    },
    {
      "when": {
        "type": "empty"
      },
      "apply": {
        "model": "immersiveposts:block/empty"
      }
    }
  ]
}