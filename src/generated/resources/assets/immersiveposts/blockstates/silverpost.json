{
  "multipart": [
    {
      "when": {
        "type": "post"
      },
      "apply": {
        "model": "immersiveposts:silverpost/post"
      }
    },
    {
      "when": {
        "type": "post_top"
      },
      "apply": {
        "model": "immersiveposts:silverpost/post_top"
      }
    },
    {
      "when": {
        "parm_north": "true"
      },
      "apply": {
        "model": "immersiveposts:silverpost/post_arm"
      }
    },
    {
      "when": {
        "parm_east": "true"
      },
      "apply": {
        "model": "immersiveposts:silverpost/post_arm",
        "y": 90
      }
    },
    {
      "when": {
        "parm_south": "true"
      },
      "apply": {
        "model": "immersiveposts:silverpost/post_arm",
        "y": 180
      }
    },
    {
      "when": {
        "parm_west": "true"
      },
      "apply": {
        "model": "immersiveposts:silverpost/post_arm",
        "y": 270
      }
    },
    {
      "when": {
        "flipstate": "up",
        "type": "arm",
        "facing": "north"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm"
      }
    },
    {
      "when": {
        "type": "arm_double",
        "facing": "north"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_double"
      }
    },
    {
      "when": {
        "flipstate": "up",
        "type": "arm",
        "facing": "east"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm",
        "y": 90
      }
    },
    {
      "when": {
        "type": "arm_double",
        "facing": "east"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_double",
        "y": 90
      }
    },
    {
      "when": {
        "flipstate": "up",
        "type": "arm",
        "facing": "south"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm",
        "y": 180
      }
    },
    {
      "when": {
        "type": "arm_double",
        "facing": "south"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_double",
        "y": 180
      }
    },
    {
      "when": {
        "flipstate": "up",
        "type": "arm",
        "facing": "west"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm",
        "y": 270
      }
    },
    {
      "when": {
        "type": "arm_double",
        "facing": "west"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_double",
        "y": 270
      }
    },
    {
      "when": {
        "flipstate": "down",
        "type": "arm",
        "facing": "north"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm",
        "x": 180,
        "y": 180
      }
    },
    {
      "when": {
        "flipstate": "down",
        "type": "arm",
        "facing": "east"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm",
        "x": 180,
        "y": 270
      }
    },
    {
      "when": {
        "flipstate": "down",
        "type": "arm",
        "facing": "south"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm",
        "x": 180
      }
    },
    {
      "when": {
        "flipstate": "down",
        "type": "arm",
        "facing": "west"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm",
        "x": 180,
        "y": 90
      }
    },
    {
      "when": {
        "flipstate": "both",
        "type": "arm",
        "facing": "north"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_twoway"
      }
    },
    {
      "when": {
        "flipstate": "both",
        "type": "arm",
        "facing": "east"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_twoway",
        "y": 90
      }
    },
    {
      "when": {
        "flipstate": "both",
        "type": "arm",
        "facing": "south"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_twoway",
        "y": 180
      }
    },
    {
      "when": {
        "flipstate": "both",
        "type": "arm",
        "facing": "west"
      },
      "apply": {
        "model": "immersiveposts:silverpost/arm_twoway",
        "y": 270
      }
    },
    {
      "when": {
        "type": "empty"
      },
      "apply": {
        "model": "immersiveposts:block/empty"
      }
    }
  ]
}